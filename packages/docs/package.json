{
  "name": "docs",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf public/**/*.{css,js,json,html} build",
    "build": "npm run clean && run-s build:*",
    "build:css": "tailwindcss -o ./app/tailwind.css --minify",
    "build:remix": "remix-ssg public",
    "dev": "npm run clean && npm run build:css && run-p dev:*",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "dev:remix": "remix dev",
    "start": "serve -l 3000 public"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.6",
    "@remix-run/react": "^1.6.6",
    "@remix-run/serve": "^1.6.6",
    "clsx": "^1.2.1",
    "front-matter": "^4.0.2",
    "highlight.js": "^11.6.0",
    "html-to-react": "^1.5.0",
    "marked": "^4.0.18",
    "node-emoji": "^1.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sanitize-html": "^2.7.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.6",
    "@remix-run/eslint-config": "^1.6.6",
    "@tailwindcss/typography": "^0.5.4",
    "@types/marked": "^4.0.3",
    "@types/node-emoji": "^1.8.1",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/sanitize-html": "^2.6.2",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "remix-flat-routes": "^0.4.3",
    "remix-ssg": "*",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
